{
  "configurations": [
    {
      "name": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Werror",
        "-lstdc++"
      ],
      "intelliSenseMode": "linux-gcc-x64"
    }
  ],
  "version": 4
}